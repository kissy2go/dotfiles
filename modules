#
# xxenv
#
type rbenv &>/dev/null && eval "$(rbenv init -)"
type nodenv &>/dev/null && eval "$(nodenv init -)"
type pyenv &>/dev/null && eval "$(pyenv init -)"

#
# Direnv
#
if type direnv &>/dev/null; then
  eval "$(direnv hook zsh)"
fi

#
# Github
#
if type hub &>/dev/null; then
  eval "$(hub alias -s)"
fi
